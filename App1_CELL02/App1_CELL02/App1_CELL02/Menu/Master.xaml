<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:page="clr-namespace:App1_CELL02.Pagina"
             x:Class="App1_CELL02.Menu.Master">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <ScrollView>
                <StackLayout>
                    <Label Text="Células"/>
                    <Button Text="Text Cell Page" Clicked="Button_Clicked" />
                    <Button Text="Image Cell Page" Clicked="Button_Clicked_1" />
                    <Button Text="Entry Cell Page" Clicked="Button_Clicked_2" />
                    <Button Text="Switch Cell Page" Clicked="Button_Clicked_3" />
                    <Button Text="View Cell Page" Clicked="Button_Clicked_4" />
                    <Label Text="Componentes"/>
                    <Button Text="ListView Page" Clicked="Button_Clicked_5" />
                    <Button Text="ListView Button Page" Clicked="Button_Clicked_6" />
                </StackLayout>
            </ScrollView>
        </ContentPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <page:TextCellPage />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>